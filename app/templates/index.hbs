<GeneralContainer>
  <div>
    <ul class="card-wrapper">
      {{#each this.productItems as |product|}}
      <LinkTo @route="item" @model={{product.id}}>
        <li class="card">
          <img src={{product.image}} alt="">
          <h3>{{product.author}}</h3>
          <p>Lorem ipsum sit dolor amit</p>
        </li>
      </LinkTo>
      {{/each}}

    </ul>

    <div class="btn-group">
      <button class="button" {{on "click" this.previousPage }} disabled={{this.disablePreviousButton}}>Previous</button>
      <button class="button" {{on "click" this.nextPage }}>Next</button>
    </div>

  </div>


</GeneralContainer>